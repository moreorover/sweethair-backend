{
    "name": "typescraper",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "compile": "tsc",
        "watch": "tsc -w",
        "dev": "nodemon dist/app.js",
        "start": "node dist/app.js",
        "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
        "typeorm:create": "npm run typeorm migration:create -- -n",
        "typeorm:generate": "npm run typeorm migration:generate -- -n",
        "typeorm:run": "npm run typeorm migration:run",
        "typeorm:revert": "npm run typeorm migration:revert",
        "typeorm:show": "npm run typeorm migration:show",
        "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
        "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/moreorover/typescript-starter.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/moreorover/typescript-starter/issues"
    },
    "homepage": "https://github.com/moreorover/typescript-starter#readme",
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/body-parser": "^1.19.1",
        "@types/connect-redis": "^0.0.17",
        "@types/express": "^4.17.13",
        "@types/express-list-endpoints": "^4.0.1",
        "@types/express-session": "^1.17.4",
        "@types/faker": "^5.5.9",
        "@types/redis": "^2.8.31",
        "del-cli": "^4.0.1",
        "eslint-plugin-tsdoc": "^0.2.14",
        "nodemon": "^2.0.12",
        "prettier": "2.3.2",
        "ts-node": "^10.4.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "argon2": "^0.28.3",
        "body-parser": "^1.19.0",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "connect-redis": "^6.0.0",
        "cors": "^2.8.5",
        "dotenv": "^8.6.0",
        "express": "^4.17.1",
        "express-list-endpoints": "^6.0.0",
        "express-session": "^1.17.2",
        "faker": "^5.5.3",
        "jsonwebtoken": "^8.5.1",
        "mysql": "^2.18.1",
        "node-cron": "^3.0.0",
        "redis": "^3.1.2",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.2.36",
        "typeorm-seeding": "^1.6.1"
    }
